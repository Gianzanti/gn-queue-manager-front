{
  "name": "gn-queue-frontend",
  "homepage": ".",
  "version": "0.0.8",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc && eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13",
    "@emotion/styled": "^11.13",
    "@mui/material": "^5.16",
    "@mui/x-data-grid": "^7.13",
    "@tanstack/react-query": "^5.51",
    "@tanstack/react-query-devtools": "^5.51",
    "axios": "^1.7",
    "clsx": "^2.1",
    "dayjs": "^1.11.12",
    "nanoid": "^5.0",
    "react": "^18.3",
    "react-dom": "^18.3",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@types/react": "^18.3",
    "@types/react-dom": "^18.3",
    "@typescript-eslint/eslint-plugin": "^7.8",
    "@typescript-eslint/parser": "^7.8",
    "@vitejs/plugin-react-swc": "^3.7",
    "eslint": "^8.57",
    "eslint-config-prettier": "^9.1",
    "eslint-plugin-react-hooks": "^4.6",
    "eslint-plugin-react-refresh": "^0.4",
    "gulp": "^4.0",
    "gulp-ssh": "^0.7",
    "husky": "^9.1",
    "prettier": "^3.3",
    "typescript": "^5.5",
    "vite": "^5.4"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 100,
    "jsxSingleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended",
      "eslint-config-prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react-refresh"
    ],
    "rules": {
      "react-refresh/only-export-components": "warn",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "vite.config.ts"
    ]
  }
}
